{
    "name": "delatbabel/site-config",
    "description": "A database backed config loader for Laravel with per-site configuration.",
    "keywords": ["laravel", "illuminate", "config", "configuration", "database", "site"],
    "license": "MIT",
    "authors": [
        {
            "name": "Del",
            "email": "del@babel.com.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "<5.4"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Delatbabel\\SiteConfig\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
